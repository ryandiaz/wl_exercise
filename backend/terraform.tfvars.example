aws_region = "us-west-1"
environment = "dev"

# Set these values with your actual API keys
openai_api_key = "your-openai-api-key-here"
fal_key = "your-fal-ai-key-here"

# Database configuration
db_password = "your-secure-database-password-here"
db_name = "imagegen"
db_username = "imagegen_user"
db_instance_class = "db.t3.micro"
db_allocated_storage = 20

# EC2 Migration Instance configuration
ec2_instance_type = "t3.micro"
# Optional: Uncomment and set to enable SSH access to the migration instance
# ec2_key_name = "your-ec2-key-pair-name"
ec2_associate_public_ip = true

# DynamoDB configuration
dynamodb_table_name = "imagegen-favorites"
dynamodb_billing_mode = "PAY_PER_REQUEST"
# Only needed if using PROVISIONED billing mode:
# dynamodb_read_capacity = 5
# dynamodb_write_capacity = 5 